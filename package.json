    "express",
    "passport"
  ],
  "author": "Osama Nabih",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OsamaNabih/Meetups/issues"
  },
  "homepage": "https://github.com/OsamaNabih/Meetups#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.1",
    "express-session": "^1.15.6",
    "joi": "^13.1.2",
    "joi-date-extensions": "^1.2.0",
    "jsonwebtoken": "^8.2.0",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
