    <title> Registration </title>
  </head>
  <body>
    <div class="container padding-top-10">
      <div class="card">
        <div class="card-header">Registration form</div>
        <div class="card-body">
          <form> <!-- when clicking on Name the first name recieves focus
            <label for="firstName" class="control-label">Name:</label> -->
            <div class="row">
              <div class="col-md-6 padding-top-10">
                  <label for="firstName" class="control-label">first name</label>
                  <input type="text" class="form-control" id="firstName"/>
              </div>
              <div class="col-md-6  padding-top-10">
                  <label for="lastName" class="control-label">last name</label>
                  <input type="text" class="form-control" id="lastName"/>
                </div>
            </div>
            <div class="row">
              <div class="col-md-12 padding-top-10">
                <label for="address1" class="control-label">Adress1</label>
                <input type="text" class="form-control" id="address1">
              </div>
              <div class="col-md-12 padding-top-10" style="padding-top: 5px">
                  <input type="text" class="form-control" id="adress2">
              </div>
            </div>
            <div class="row padding-top-10">
                <div class="col-md-6 ">

                </div>
                <div class="col-md-2 "></div>


                <div class="col-md-4 ">

                </div>
            </div>
            <div class="row padding-top-10">
              <div class="col-md-6 padding-top-10">
                  <label for="city" class="control-label">City</label>
                  <input type="text" class="form-control" id="city">
              </div>
              <div class="col-md-2 padding-top-10">
                  <label for="zipCode" class="control-label">zip code</label>
                  <input type="text" class="form-control" id="zipCode">
              </div>
              <div class="col-md-4 padding-top-10">
                  <label for="crap" class="control-label">crap</label>
                  <input type="crap" class="form-control" id="crap">
              </div>
            </div>
            <div class="row">
              <div class="col-md-6 padding-top-10">
                <label for="email" class="control-label">Email Address</label>
                <input type="text" class="form-control" id="email" placeholder="Email address"/>
              </div>
              <div class="col-md-6 padding-top-10">
                  <label for="communication" class="control-label">communication</label>
                  <select id="communication" class="form-control">
                    <option value="text"> text</option>
                    <option value="text"> html</option>
                  </select>
                </div>
            </div>
           <div class="row">
             <div class="col-md-6 padding-top-10">
               <label for="password" class="control-label">Password</label>
               <input type="password" class="form-control" id="password" placeholder="Enter password"/>
             </div>
             <div class="col-md-6 padding-top-10">
                <label for="confirmPassword" class="control-label">Confirm Password</label>
                <input type="password" class="form-control" id="confirmPassword" placeholder="Confirm password"/>
              </div>
           </div>
           <div class="row">
             <div class="col-md-6 padding-top-10">
               <div class="checkbox">
                 <label>
                   <input type="checkbox"/> Click here to subscribe
                 </label>
               </div>
             </div>
           </div>
           <div class="row">
             <div class="col-md-2">
               <button type="button" class="btn btn-elegant">register</buttton>
             </div>
           </div>

          </form>
        </div>
      </div>

    </div>




